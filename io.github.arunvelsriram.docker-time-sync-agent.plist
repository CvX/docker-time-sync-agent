<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>Label</key>
		<string>io.github.arunvelsriram.docker-time-sync-agent</string>
		<key>Program</key>
		<string>/usr/local/bin/docker-time-sync-agent</string>
		<key>ProgramArguments</key>
		<array>
			<string>docker-time-sync-agent</string>
		</array>
		<key>KeepAlive</key>
		<true/>
		<key>StandardOutPath</key>
		<string>/Users/YOUR_USERNAME/.docker-time-sync-agent.log</string>
		<key>StandardErrorPath</key>
		<string>/Users/YOUR_USERNAME/.docker-time-sync-agent.log</string>
	</dict>
</plist>
